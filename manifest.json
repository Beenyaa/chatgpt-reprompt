{
  "name": "ChatGPT: RePrompt",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.chat.openai.com/*"],
      "js": ["./src/main.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}